{
  "settings": {
    "analysis": {
      "filter": {
        "nGram_filter": {
          "token_chars": [
            "letter",
            "digit",
            "punctuation",
            "symbol"
          ],
          "min_gram": "1",
          "type": "nGram",
          "max_gram": "20"
        }
      },
      "analyzer": {
        "nGram_analyzer": {
          "filter": [
            "lowercase",
            "asciifolding",
            "nGram_filter"
          ],
          "type": "custom",
          "tokenizer": "whitespace"
        },
        "whitespace_analyzer": {
          "filter": [
            "lowercase",
            "asciifolding"
          ],
          "type": "custom",
          "tokenizer": "whitespace"
        }
      }
    }
  },
  "mappings": {
    "openhome": {
      "include_in_all": false,
      "_all": {
        "enabled": true,
        "analyzer": "nGram_analyzer",
        "search_analyzer": "whitespace_analyzer"
      },
      "dynamic_templates": [
        {
          "notanalyzed": {
            "mapping": {
              "index": "not_analyzed",
              "type": "string"
            },
            "match": "*",
            "match_mapping_type": "string",
            "unmatch": "*description*"
          }
        }
      ],
      "properties": {
        "_listingId": {
          "type": "string",
          "index": "not_analyzed"
        },
        "_mls": {
          "type": "string",
          "index": "not_analyzed"
        },
        "_modified": {
          "type": "date",
          "format": "strict_date_optional_time||epoch_millis",
          "include_in_all": false
        },
        "comments": {
          "type": "string",
          "index": "not_analyzed",
          "include_in_all": false
        },
        "endDateTime": {
          "type": "date",
          "format": "strict_date_optional_time||epoch_millis",
          "include_in_all": false
        },
        "openHouseRID": {
          "type": "string",
          "index": "not_analyzed",
          "include_in_all": false
        },
        "startDateTime": {
          "type": "date",
          "format": "strict_date_optional_time||epoch_millis",
          "include_in_all": false
        }
      }
    }
  }
}