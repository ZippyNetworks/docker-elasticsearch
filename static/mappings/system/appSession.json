{
  "appSession": {
    "dynamic_templates": [
      {
        "notanalyzed": {
          "mapping": {
            "index": "not_analyzed",
            "type": "string"
          },
          "match": "*",
          "match_mapping_type": "string"
        }
      }
    ],
    "properties": {

      "_token": {
        "type": "string",
        "index": "not_analyzed"
      },

      "_created": {
        "type": "date",
        "format": "dateOptionalTime"
      },

      "userAgent": {
        "type": "string",
        "index": "not_analyzed"
      },

      "ip": {
        "type": "string",
        "index": "not_analyzed"
      },

      "host": {
        "type": "string",
        "index": "not_analyzed"
      },

      "brand": {
        "type": "string",
        "index": "not_analyzed"
      },

      "subdomain": {
        "type": "string",
        "index": "not_analyzed"
      }

    }
  }
}